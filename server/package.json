{
  "name": "signalbox-llm-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test-agent": "ts-node scripts/test-agent.ts",
    "test-currency": "ts-node scripts/test-currency.ts",
    "deploy:build": "pnpm install --frozen-lockfile && pnpm run build",
    "deploy:docker:build": "docker build -t signalbox-llm-api .",
    "deploy:docker:run": "docker run -p 3000:3000 --env-file .env signalbox-llm-api",
    "deploy:test": "pnpm run build && pnpm run start:prod",
    "deploy:apprunner:update": "aws apprunner update-service --cli-input-json file://apprunner-update.json",
    "secret:create:ps": "powershell -Command \"try { aws secretsmanager create-secret --name $env:SECRET_NAME --secret-string $env:SECRET_VALUE --region us-east-1 --query 'ARN' --output text } catch { aws secretsmanager update-secret --secret-id $env:SECRET_NAME --secret-string $env:SECRET_VALUE --region us-east-1 --query 'ARN' --output text }\""
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "^6.4.0",
    "@scalar/nestjs-api-reference": "^0.5.9",
    "@types/eventsource": "^3.0.0",
    "@types/ioredis": "^5.0.0",
    "@upstash/redis": "^1.35.1",
    "axios": "^1.10.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cli-progress": "^3.12.0",
    "dotenv": "^17.2.0",
    "eventsource": "^4.0.0",
    "ioredis": "^5.6.1",
    "moment": "^2.30.1",
    "nanospinner": "^1.2.2",
    "nestjs-real-ip": "^3.0.1",
    "openai": "^5.9.0",
    "reflect-metadata": "^0.2.2",
    "remark-gfm": "^4.0.1",
    "rxjs": "^7.8.1",
    "tiktoken": "^1.0.21",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/cli-progress": "^3.11.6",
    "@types/express": "^5.0.0",
    "@types/inquirer": "^9.0.8",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "chalk": "^5.4.1",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "inquirer": "^12.7.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "packageManager": "pnpm@8.15.4+sha512.0bd3a9be9eb0e9a692676deec00a303ba218ba279d99241475616b398dbaeedd11146f92c2843458f557b1d127e09d4c171e105bdcd6b61002b39685a8016b9e"
